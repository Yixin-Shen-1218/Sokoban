<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="630.0" prefWidth="600.0" style="-fx-background-image: url('/img/Menu.jpg');" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ae2dms.Controllers.MenuController">
   <children>
      <ImageView fitHeight="630.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/BackGround/Menu.png" />
         </image>
      </ImageView>
      <Button fx:id="Start" layoutX="196.0" layoutY="196.0" mnemonicParsing="false" onAction="#startGame" prefHeight="34.0" prefWidth="207.0" text="START THE GAME" />
      <Button fx:id="Setting" layoutX="196.0" layoutY="255.0" mnemonicParsing="false" onAction="#openSetting" prefHeight="34.0" prefWidth="207.0" text="SETTING" />
      <Button fx:id="Ranking" layoutX="196.0" layoutY="368.0" mnemonicParsing="false" onAction="#openRank" prefHeight="34.0" prefWidth="207.0" text="RANKING" />
      <Button fx:id="Exit" layoutX="196.0" layoutY="424.0" mnemonicParsing="false" onAction="#closeGame" prefHeight="34.0" prefWidth="207.0" text="EXIT" />
      <Button fx:id="Help" layoutX="196.0" layoutY="310.0" mnemonicParsing="false" onAction="#openHelp" prefHeight="34.0" prefWidth="207.0" text="HELP" />
      <Button fx:id="Tutorial" layoutX="196.0" layoutY="541.0" mnemonicParsing="false" onAction="#openTutorial" prefHeight="34.0" prefWidth="207.0" text="TUTORIAL" />
   </children>
</AnchorPane>
