<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="630.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ae2dms.Controllers.SlotController">
   <ImageView fitHeight="630.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../img/BackGround/Slot.png" />
      </image>
   </ImageView>
    <Button fx:id="But1" layoutX="196.0" layoutY="196.0" mnemonicParsing="false" onAction="#openSlot1" prefHeight="34.0" prefWidth="207.0" text="SLOT 1" />
    <Button fx:id="But2" layoutX="196.0" layoutY="255.0" mnemonicParsing="false" onAction="#openSlot2" prefHeight="34.0" prefWidth="207.0" text="SLOT 2" />
    <Button fx:id="But3" layoutX="196.0" layoutY="310.0" mnemonicParsing="false" onAction="#openSlot3" prefHeight="34.0" prefWidth="207.0" text="SLOT 3" />
    <Button fx:id="Erase" layoutX="196.0" layoutY="368.0" mnemonicParsing="false" onAction="#eraseSlot" prefHeight="34.0" prefWidth="207.0" text="ERASE SLOT" />
    <Button fx:id="Return" layoutX="196.0" layoutY="424.0" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="34.0" prefWidth="207.0" text="RETURN" />
</AnchorPane>
